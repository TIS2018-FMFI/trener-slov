<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane fx:id="pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
   <left>
      <Label fx:id="groupName" minWidth="100.0" BorderPane.alignment="CENTER">
         <padding>
            <Insets left="20.0" />
         </padding>
      </Label>
   </left>
   <right>
      <HBox spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="editBtn" mnemonicParsing="false" text="Upraviť" />
            <Button fx:id="deleteBtn" mnemonicParsing="false" text="Zmazať" />
         </children>
         <padding>
            <Insets right="20.0" />
         </padding>
      </HBox>
   </right>
</BorderPane>
