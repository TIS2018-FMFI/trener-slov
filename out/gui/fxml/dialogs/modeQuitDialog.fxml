<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.dialogControllers.ModeQuitDialogController">
   <children>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="50.0">
         <children>
            <Label fx:id="label" text="Mód skončil. " />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="50.0">
         <children>
            <Button fx:id="quitBtn" mnemonicParsing="false" text="Skončiť">
               <FlowPane.margin>
                  <Insets right="10.0" />
               </FlowPane.margin>
            </Button>
            <Button fx:id="againBtn" mnemonicParsing="false" text="Spustiť znovu">
               <FlowPane.margin>
                  <Insets left="10.0" />
               </FlowPane.margin>
            </Button>
         </children>
      </FlowPane>
   </children>
</VBox>
