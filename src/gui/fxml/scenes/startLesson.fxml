<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.sceneControllers.StartLessonController">
   <top>
   </top>
   <bottom>
      <BorderPane prefHeight="50.0" BorderPane.alignment="CENTER">
         <right>
            <Button fx:id="startBtn" mnemonicParsing="false" text="Spustiť" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" right="20.0" />
               </BorderPane.margin>
            </Button>
         </right>
         <left>
            <Button fx:id="backBtn" minWidth="100" mnemonicParsing="false" text="Späť" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="20.0" left="20.0" />
               </BorderPane.margin>
            </Button>
         </left>
         <BorderPane.margin>
            <Insets top="20.0" />
         </BorderPane.margin>
      </BorderPane>

   </bottom>
   <center>
      <ListView fx:id="listView" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
